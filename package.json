{
    "name": "svelte-div-100vh",
    "version": "0.1.0",
    "description": "Experimental Svelte port of react-div-100vh",
    "main": "index.js",
    "author": "Samuel Plumppu",
    "license": "MIT",
    "dependencies": {
        "lodash-es": "^4.17.21",
        "svelte": "^3.38.2"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
        "prettier-plugin-svelte": "^2.3.0",
        "vite": "^2.3.7"
    },
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "serve": "vite preview"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
